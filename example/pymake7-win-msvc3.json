            "set src=%src_path%/QQt.pro",
            "set build=${root.build}/QQt/${QSYS}/${QTVERSION}/Debug",
            "md %build:/=\\%",
            "cd /d %build%",
            "del /f /q /s demo examples src test",
            "qmake %src% ${QTSPEC} CONFIG+=debug CONFIG+=qml_debug ${QTCONFIG} && mingw32-make qmake_all",
